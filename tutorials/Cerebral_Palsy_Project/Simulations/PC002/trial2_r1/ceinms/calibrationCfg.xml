<?xml version="1.0" ?>
<calibration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   <algorithm>
      <simulatedAnnealing>
         <noEpsilon>4</noEpsilon>
         <rt>0.3</rt>
         <T>20</T>
         <NS>15</NS>
         <NT>5</NT>
         <epsilon>1.E-5</epsilon>
         <maxNoEval>2000</maxNoEval>
      </simulatedAnnealing>
   </algorithm>
   <NMSmodel>
      <type>
         <openLoop/>
      </type>
      <tendon>
         <equilibriumElastic/>
      </tendon>
      <activation>
         <exponential/>
      </activation>
   </NMSmodel>
   <calibrationSteps>
      <step>
         <dofs>hip_flexion_r hip_adduction_r hip_rotation_r knee_angle_r ankle_angle_r</dofs>
         <objectiveFunction>
            <torqueErrorNormalised>
               <targets>all</targets>
               <weight>1</weight>
               <exponent>1</exponent>
            </torqueErrorNormalised>
            <penalty>
               <targets>all</targets>
               <targetsType>normalisedFibreLength</targetsType>
               <weight>100</weight>
               <exponent>2</exponent>
               <range>0.6 1.4</range>
            </penalty>
         </objectiveFunction>
         <parameterSet>
            <parameter>
               <name>c1</name>
               <single/>
               <absolute>
                  <range>-0.95 -0.05</range>
               </absolute>
            </parameter>
            <parameter>
               <name>c2</name>
               <single/>
               <absolute>
                  <range>-0.95 -0.05</range>
               </absolute>
            </parameter>
            <parameter>
               <name>shapeFactor</name>
               <single/>
               <absolute>
                  <range>-2.999 -0.001</range>
               </absolute>
            </parameter>
            <parameter>
               <name>tendonSlackLength</name>
               <single/>
               <relativeToSubjectValue>
                  <range>0.85 1.15</range>
               </relativeToSubjectValue>
            </parameter>
            <parameter>
               <name>optimalFibreLength</name>
               <single/>
               <relativeToSubjectValue>
                  <range>0.85 1.15</range>
               </relativeToSubjectValue>
            </parameter>
            <parameter>
               <name>strengthCoefficient</name>
               <single/>
               <absolute>
                  <range>0.8 2</range>
               </absolute>
            </parameter>
         </parameterSet>
      </step>
   </calibrationSteps>
   <trialSet>.\trial.xml</trialSet>
</calibration>
